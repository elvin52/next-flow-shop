{
  "headers": [
    {
      "source": "/(men|women)/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex, nofollow"
        }
      ]
    },
    {
      "source": "/product/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex, nofollow"
        }
      ]
    },
    {
      "source": "/(cart|checkout|account|products)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex, nofollow"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/men",
      "has": [
        {
          "type": "query",
          "key": "color"
        }
      ],
      "destination": "/men?legacy=1",
      "statusCode": 301
    },
    {
      "source": "/women",
      "has": [
        {
          "type": "query",
          "key": "color"
        }
      ],
      "destination": "/women?legacy=1",
      "statusCode": 301
    },
